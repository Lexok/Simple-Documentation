<?xml version="1.0"?>
<ruleset name="Custom Standard">
    <rule ref="PSR2">
        <exclude name="Generic.Files.LineLength"/>
    </rule>

    <rule ref="Generic.Files.LineLength">
        <severity>0</severity>
    </rule>

    <rule ref="Generic.Files.LineLength.TooLong">
        <severity>0</severity>
    </rule>
</ruleset>
