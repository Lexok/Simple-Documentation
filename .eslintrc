{
    "env": {
        "browser": true,
        "jquery": true
    },
    "rules": {
        "indent": [2, 4, { "SwitchCase": 1 }],
        "one-var": [2, {
            "var": "always",
            "let": "always",
            "const": "never"
        }],
        "semi": [2, "always"],
        "space-before-function-paren": [2, "never"],
        "no-multiple-empty-lines": [2, { "max": 2 }],
        "no-unused-vars": [1, { "vars": "all", "args": "none" }]
    },
    "extends": "standard"
}
